{
  "name": "react-framework",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "lint": "yarn _typeCheck && yarn _tsLint",
    "test": "jest ./test",
    "_typeCheck": "tsc --project tsconfig.json --noEmit",
    "_tsLint": "tslint -p ./tsconfig.json ./src/**/*.{ts,tsx}"
  },
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/src/**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "setupTestFrameworkScriptFile": "./test/enzyme.setup.ts",
    "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx|jsx|js)$",
    "moduleNameMapper": {
      "\\.(less|scss|css|jpg|png|svg)$": "jest-css-modules",
      "^@config(.*)$": "<rootDir>/src/config$1",
      "^@components(.*)$": "<rootDir>/src/components$1",
      "^@pages(.*)$": "<rootDir>/src/pages$1",
      "^@utils(.*)$": "<rootDir>/src/utils$1",
      "^@layouts(.*)$": "<rootDir>/src/layouts$1",
      "^@services(.*)$": "<rootDir>/src/services$1",
      "^@assets(.*)$": "<rootDir>/src/assets$1"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": false
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn lint"
    }
  },
  "standard": {
    "globals": [
      "console",
      "Error",
      "config",
      "$",
      "antd",
      "woff"
    ],
    "properties": false,
    "parser": "babel-eslint",
    "ignore": [
      "./public/dist/",
      "./build/"
    ]
  },
  "dependencies": {
    "@types/react-router-dom": "^4.3.1",
    "antd": "^3.10.7",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "less": "^3.8.1",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@types/enzyme": "^3.1.14",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.5",
    "@types/react": "^16.7.3",
    "@types/react-dom": "^16.0.9",
    "@types/react-loadable": "^5.4.1",
    "autoprefixer": "^9.3.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-preset-react-hmre": "^1.1.1",
    "browserslist": "^4.3.4",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-hot-loader": "^1.4.2",
    "css-loader": "^1.0.1",
    "cssnano": "^4.1.7",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "file-loader": "^2.0.0",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.3",
    "jest": "^23.6.0",
    "jest-canvas-mock": "^1.1.0",
    "jest-css-modules": "^1.1.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.4",
    "postcss-color-rgba-fallback": "^3.0.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-flexibility": "^2.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-opacity": "^5.0.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react-test-renderer": "^16.6.3",
    "resolve-url-loader": "^3.0.0",
    "ts-import-plugin": "^1.5.5",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.6",
    "url-loader": "^1.1.2",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-parallel-uglify-plugin": "^1.1.0"
  }
}
